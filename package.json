{
  "name": "svelte-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "prepare": "svelte-kit sync",
    "start": "vite dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "i18n:extract": "svelte-i18n extract 'src/**/*.svelte'",
    "msw": "pnpm dlx msw init"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.43.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "svelte": "^5.38.10",
    "svelte-check": "^4.3.1",
    "svelte-i18n": "^4.0.1",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "cookie": "^1.0.2",
    "esbuild": "^0.25.0"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  },
  "pnpm": {
    "overrides": {
      "devalue": "5.3.2"
    },
    "onlyBuiltDependencies": [
      "es5-ext",
      "esbuild",
      "msw"
    ]
  }
}