{
  "name": "svelte-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "yarn@4.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "svelte-check",
    "i18n:extract": "svelte-i18n extract 'src/**/*.svelte'"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "rxjs": "^7.8.1",
    "svelte-routing": "^2.5.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.4",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.9",
    "@vitest/coverage-v8": "^0.34.6",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "jsdom": "^22.1.0",
    "msw": "^2.0.1",
    "sass": "^1.69.5",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-forms-lib": "^2.0.1",
    "svelte-htm": "^1.2.0",
    "svelte-i18n": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "~5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
