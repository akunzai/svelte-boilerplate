{
  "name": "svelte-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "prepare": "svelte-kit sync",
    "start": "vite dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "i18n:extract": "svelte-i18n extract 'src/**/*.svelte'"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "svelte": "^5.37.2",
    "svelte-check": "^4.2.2",
    "svelte-i18n": "^4.0.1",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "cookie": "^1.0.1",
    "rollup": "4.24.0"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "es5-ext",
      "esbuild",
      "msw"
    ]
  }
}