{
  "name": "svelte-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "prepare": "svelte-kit sync",
    "start": "vite dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "i18n:extract": "svelte-i18n extract 'src/**/*.svelte'"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.7.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.8.1",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "2.1.3",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "jsdom": "^25.0.1",
    "msw": "^2.5.1",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.5",
    "svelte-i18n": "^4.0.1",
    "tslib": "^2.8.0",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  },
  "resolutions": {
    "cookie": "^1.0.1",
    "rollup": "4.24.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}