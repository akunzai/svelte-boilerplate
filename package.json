{
  "name": "svelte-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "svelte-check",
    "i18n:extract": "svelte-i18n extract 'src/**/*.svelte'"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "rxjs": "^7.8.1",
    "svelte-routing": "^2.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/svelte": "^4.0.3",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/svelte": "^5.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@vitest/coverage-v8": "^0.34.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "sass": "^1.64.0",
    "svelte": "^4.1.0",
    "svelte-check": "^3.4.6",
    "svelte-forms-lib": "^2.0.1",
    "svelte-htm": "^1.2.0",
    "svelte-i18n": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.5",
    "vitest": "^0.34.0"
  },
  "resolutions": {
    "semver": "^7.5.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
